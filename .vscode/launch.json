        "version": "0.2.0",
        "configurations": [
                {
                        "type": "lldb",
                        "request": "launch",
                        "name": "Debug",
                        "program": "${workspaceFolder}/build/bin/local/radiusd",
                        "args": [
                                "-X",
                                "-D",
                                "${workspaceFolder}/share/dictionary",
                                "-d",
                                "${workspaceFolder}/raddb"
                        ],
                        "cwd": "${workspaceFolder}",
                        "env": {
                                "FR_LIBRARY_PATH": "${workspaceFolder}/build/lib/local/.libs"
                        }
                }
        ]
}
