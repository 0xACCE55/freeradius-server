&Tmp-Group-0.NAS-Port := { 1, 3, 5, 7, 11 }
&Port-Limit := 0

#
#  Do operations on sets of inputs.
#
&Port-Limit += &Tmp-Group-0.NAS-Port[*]
if (!(&Port-Limit == 27)) {
	test_fail
}

&Tmp-Group-1 := &Tmp-Group-0
&Port-Limit := 0

&Port-Limit += &Tmp-Group-1.NAS-Port[*]
if (!(&Port-Limit == 27)) {
	test_fail
}

success
