#
#  PRE: update
#
parallel {
	update parent.control {
	       Tmp-Integer-0 += 1
	}
	update parent.control {
	       Tmp-Integer-0 += 2
	}
	update parent.control {
	       Tmp-Integer-0 += 3
	}
	update parent.control {
	       Tmp-Integer-0 += 4
	}
}

if ("%{control:Tmp-Integer-0[#]}" != 4) {
	update reply {
		Filter-Id += "fail-count"
	}

	return
}

success
