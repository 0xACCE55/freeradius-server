TARGET      = @targetname@
SRCS        = rlm_eap_peap.c peap.c
HEADERS     = eap_peap.h ../../eap.h ../../rlm_eap.h
RLM_CFLAGS  = $(INCLTDL) -I../.. $(OPENSSL_INCLUDE) -I../../libeap -DOPENSSL_NO_KRB5 @eap_peap_cflags@
