TARGETNAME	:= @targetname@

ifneq "$(TARGETNAME)" ""
TARGET		:= $(TARGETNAME).a
endif

SOURCES		:= $(TARGETNAME).c mem.c

SRC_CFLAGS	:= @mod_cflags@
TGT_LDLIBS	:= @mod_ldflags@

ifneq "$(TARGETNAME)" ""
install: $(R)$(raddbdir)/mods-available/securid

$(R)$(raddb)/mods-available/securid: src/modules/rlm_securid/securid | $(R)$(raddb)/mods-available
	@$(ECHO) INSTALL $(notdir $<)
	@$(INSTALL) -m 640 $< $(R)$(raddb)/mods-available
endif
